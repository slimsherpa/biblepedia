{
  "name": "template-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "powershell -NoProfile -ExecutionPolicy Bypass -File cleanup.ps1",
    "build": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"& {./cleanup.ps1; if ($?) { next build }}\"",
    "start": "next start",
    "lint": "next lint",
    "deploy": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"& {./cleanup.ps1; if ($?) { next build; if ($?) { firebase deploy }}}\"",
    "deploy:hosting": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"& {./cleanup.ps1; if ($?) { next build; if ($?) { firebase deploy --only hosting }}}\"",
    "deploy:functions": "firebase deploy --only functions"
  },
  "dependencies": {
    "@deepgram/sdk": "^3.6.0",
    "date-fns": "^3.6.0",
    "firebase": "^11.4.0",
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2",
    "lucide-react": "^0.436.0",
    "marked": "^15.0.7",
    "next": "^15.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "replicate": "^0.32.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/marked": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.7",
    "postcss": "^8",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "projects": {
    "default": "biblepediaio"
  },
  "targets": {
    "biblepediaio": {
      "hosting": {
        "biblepediaio": [
          "biblepediaio"
        ]
      }
    }
  }
}
